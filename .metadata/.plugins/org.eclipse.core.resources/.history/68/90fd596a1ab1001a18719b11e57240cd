package com.java.practice;

public class DivisibleBy {

	public static void main(String[] args) {
		int count = 0;
		for (int number = 0; number < 100; number++) {
			while (count < 5) {
				if (((number % 2) == 0) && ((number % 3) == 0) && ((number % 2) == 5)) {
					count++;
					break;
				}

			}
		}

	}

}
